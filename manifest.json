{
    "manifest_version": 3,
    "name": "Tab Exporter",
    "version": "1.0.0",
    "description": "Export all open tabs' titles and URLs",
    "permissions": ["tabs", "storage"],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    }
}